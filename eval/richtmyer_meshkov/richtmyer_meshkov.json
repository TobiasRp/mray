{
  "dataset": "data/richtmyer_meshkov.nc",
  "output": "results/",
  "artifacts": "[reference, reconstruct, error-bounds, fourier]",
  "perf_logging": true,

  "common": {
    "preintegration_step_size_factor": 0.25,
    "step_size": 1.0,
    "step_size_write_samples": 8.0,
    "bias": 1e-5
  },

  "reference": {
    "width": 1024,
    "height": 768,
    "early_ray_termination": 0.95,
    "medium_emission": 1.0
  },

  "generation": {
    "width": 1024,
    "height": 768,
    "num_moments": 100,
    "transform_to_bounds": 1,
    "transform_bound_eps": 0.005,
    "compact_image": 1,
    "prediction_coding": 1,
    "coding_warp": 0,
    "entropy_coding": 1,
    "compress": 1,
    "quantization_bits": 10,
    "error_threshold": 0.01
  },

  "reconstruction": {
    "early_ray_termination": 0.95,
    "medium_emission": 1.0,
    "medium_scattering": 0.5
  },

  "reconstruction_fourier": {
  },

  "error_bounds": {
  },

  "frames": {
    "frame": {
      "camera": "eval/richtmyer_meshkov/config/camera.txt",
      "tf": "eval/richtmyer_meshkov/config/tf.bin",
      "t": 0,
      "variable": "s"
    }
  }
}
